{
  "name": "frontend",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --env-file ../../.env --hot src/index.html",
    "start": "NODE_ENV=production bun run --env-file ../../.env src/index.html --host=0.0.0.0",
    "build": "bun run --env-file ../../.env build.ts --env 'BUN_PUBLIC_*' --public-path /"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "axios": "^1.13.2",
    "bun-plugin-tailwind": "^0.1.2",
    "daisyui": "^5.5.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.10.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/bun": "latest",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3"
  }
}
